    "p-reduce": "3.0.0",
    "readdirp": "4.1.2",
    "semver": "7.7.2",
    "top-user-agents": "2.1.68"
  },
  "devDependencies": {
    "@types/node": "24.3.1",
    "@types/p-map": "2.0.0",
    "@types/semver": "7.7.1",
    "eslint": "9.34.0",
    "eslint-config-upleveled": "9.2.14",
    "p-map": "7.0.3",
